{
  "expo": {
    "name": "vitta",
    "slug": "vitta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "vitta",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "router": {
      "root": "src/app"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gabriel_silva472.vitta"
    },
    "web": {
      "output": "static"
    },
    "splash": {
      "image": "./src/assets/images/logo_vitta.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "988793c4-1eed-4e88-980d-ccd966e3b462"
      }
    }
  }
}
